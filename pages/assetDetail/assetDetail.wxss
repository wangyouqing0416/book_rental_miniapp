page {
  background-color: #f3f3f3;
}

.one {
  text-align: center;
  color: #000;
  font-size: 12px;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 26rpx;
  background: white;
}

.one::after {
  content: "";
  width: 100%;
  height: 1px;
  background-color: #e5e5e5;
  position: absolute;
  bottom: 0;
  z-index: 1;
  left: 0;
}

.viewinfo > view {
  padding: 5rpx 0rpx;
  text-align: left;
}

.money {
  font-size: 45rpx;
}
